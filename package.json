{
  "name": "slacknametags",
  "version": "1.0.0",
  "description": "A Slack App for Generating Name Tags from Slack Info",
  "type": "module",
  "main": "./dist/app.js",
  "exports": {
    ".": "./dist/app.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "scaffold": "rm -rf ./dist && mkdir dist && cp -R fonts dist && cp -R images dist",
    "build": "npm run scaffold && tsc",
    "script": "npm run build && node --experimental-fetch ./dist/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Tiffany Forkner",
  "license": "ISC",
  "dependencies": {
    "@slack/bolt": "^3.13.0",
    "@slack/web-api": "^6.8.1",
    "csv-parse": "^5.3.8",
    "pdfkit": "^0.13.0",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@types/pdfkit": "^0.12.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
